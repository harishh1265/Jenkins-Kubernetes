pipeline {
    agent any 
        stages {
            stage ('Exicuting parallel job') {
                steps {
                    echo "build step need to be writtrn to build a project"
                    sleep 5
                      }
                  }
                  
             stage ('writing a parallel exicution job') {
                parallel {
                    stage ('Build') {
                        steps {
                            git branch: 'main', url: 'https://github.com/suman4197/java-project.git'
                            sh 'mvn clean install'
                            echo "build is succesful"
                            sleep 5
                               }
                            }
                       stage ('Deploy') {
                            steps {
                                  'cp /var/lib/jenkins/.m2/repository/com/efsavage/hello-world-war/1.0.0/hello-world-war-1.0.0.war /home/ec2-user/tomcat/apache-tomcat-8.5.73/webapps'
                                  echo "deploy is succesful"
                                  sleep 5
                                  }
                             }
                        }
                   }
                 stage ('Test') {
                        steps { 
                            echo "test is succesful"
                            sleep 5
                                }
                             }
                   }
           }
                            
                            
